<script>
        var vid, idClicked;
        $('.video-buttons').click(function(event){
            event.preventDefault();
            idClicked = $(this).attr('id');
        });
        $('.closePopup').click(function(event){
            event.preventDefault();
            $('#popup-'+idClicked).css('display','none');
            $('.navbar-default').css('display', 'block');
            $('#video-'+idClicked).get(0).pause();
        });
</script>
